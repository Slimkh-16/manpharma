{{ header }} 
{{ column_left }} 
<div id="content">
    <a id="topPage"></a>
    <div class="page-header">
        <div class="container-fluid">
            <div class="pull-right">
                <button type="submit" form="form-item" data-toggle="tooltip" title="{{ button_save }}" class="btn btn-primary"><i class="fa fa-save"></i>
                </button> <a href="{{ cancel }}" data-toggle="tooltip" title="{{ button_cancel }}" class="btn btn-default"><i class="fa fa-reply"></i></a>
            </div>
            <h1>{{ heading_title }}</h1>
            <ul class="breadcrumb">
                {% for breadcrumb in breadcrumbs %} 
                <li>
                    <a href="{{ breadcrumb['href'] }}">
                        {{ breadcrumb['text'] }} 
                    </a>
                </li>
                {% endfor %} 
            </ul>
        </div>
    </div>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/css/bootstrap.min.css">

    <div class="container-fluid">
        {% if (error_warning) %} 
        <div class="alert alert-danger"><i class="fa fa-exclamation-circle"></i> 
            {{ error_warning }} 
            <button type="button" class="close" data-dismiss="alert">&times;</button>
        </div>
        {% endif %} 
        <div class="panel panel-default">
            <div class="panel-heading">
                <h3 class="panel-title"><i class="fa fa-pencil"></i> Edit Cache Tester </h3>
            </div>
            <div class="panel-body">
                <form action="{{ action }}" method="post" enctype="multipart/form-data" id="form-item" class="form-horizontal">
                    <div class="tab-content">

                        <div class="tab-pane active" id="0">
                            <div class="setting">
                                <div class="text-info text-left pad-bottom">

                                </div>
                            </div>
                            <div class="lead well well-sm text-info ">
                                <div class="pull-right"></div>    <small>Options</small>
                            </div>
                            <div class="form-group">
                                <label class="col-sm-3 control-label" for="input-status">
                                    Click the button. This sends a redirect flag to the server via the querystring. The server will remove the flag from the querystring and redirect back to this same page.
                                </label>
                                <div class="col-sm-9">
                                   <a href="{{ redirect }}" data-toggle="tooltip" title="Redirect" class="btn btn-success"><i class="fa fa-plus-circle"></i></a>
                                </div>
                            </div>

                        </div>


                </form>
                </div>
            </div>
        </div>
    </div>
{{ footer }}